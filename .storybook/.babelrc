{
  "presets": [
      [
        "@babel/preset-env",
        {
          "loose": true,
          "modules": false
        }
      ],
      "@babel/preset-react"
  ],
  "plugins": [
    "@babel/plugin-proposal-object-rest-spread",
    "@babel/plugin-transform-object-assign",
    "@babel/plugin-transform-react-jsx"
  ]
}
